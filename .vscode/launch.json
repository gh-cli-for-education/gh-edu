  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/gh-edu.ts",
      "runtimeArgs": [ "-r", "ts-node/register", "-r", "tsconfig-paths/register" ],
      "args": [ "get", "-o" ],
      "outFiles": [
        "${workspaceFolder}/js/**/*.js"
      ]
    }
  ]
}